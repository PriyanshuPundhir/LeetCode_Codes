class Solution {
public:
    void merge(vector<int>& nums1, int m, vector<int>& nums2, int n) 
    {
        vector<int> res;
        
        
         for(int i=m;i<m+n;i++){
        nums1[i]=nums2[i-m];
    }
       
        sort(nums1.begin(),nums1.end());
        for(int i =0 ; i<nums1.size();i++)
        {
            cout<<nums1[i];
        }
        
       
    }
};
